# CampusKinect Web Frontend - PRODUCTION Environment Example
# Copy this file to .env.production and update the values for your production deployment

# =============================================================================
# CORE API CONFIGURATION - UPDATE THESE WITH YOUR PRODUCTION DOMAINS
# =============================================================================
NEXT_PUBLIC_API_URL=https://api.campuskinect.net/api/v1
NEXT_PUBLIC_WS_URL=wss://api.campuskinect.net

# =============================================================================
# AUTHENTICATION & SECURITY
# =============================================================================
NEXT_PUBLIC_JWT_STORAGE_KEY=ck_prod_token
NEXT_PUBLIC_REFRESH_TOKEN_KEY=ck_prod_refresh

# =============================================================================
# APPLICATION FEATURES
# =============================================================================
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_DEBUG_MODE=false
NEXT_PUBLIC_MAX_FILE_SIZE=10485760

# =============================================================================
# EXTERNAL SERVICES - UPDATE WITH YOUR PRODUCTION KEYS
# =============================================================================
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your_production_cloudinary
NEXT_PUBLIC_CLOUDINARY_UPLOAD_PRESET=campuskinect_uploads
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_production_google_maps_key

# =============================================================================
# DEPLOYMENT CONFIGURATION
# =============================================================================
NEXT_PUBLIC_APP_URL=https://campuskinect.net
NEXT_PUBLIC_CDN_URL=https://cdn.campuskinect.net
NEXT_PUBLIC_ENVIRONMENT=production
NEXT_PUBLIC_APP_VERSION=1.0.0 